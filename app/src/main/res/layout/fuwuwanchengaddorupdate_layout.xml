<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yuyuebianhao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="预约编号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yuyuebianhao_et"
            style="@style/add_item_input_style"
            android:hint="预约编号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/fuwumingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="服务名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/fuwumingcheng_et"
            style="@style/add_item_input_style"
            android:hint="服务名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            android:id="@+id/fengmian_ll"
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:id="@+id/fengmian_tv"
                android:text="封面"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyImageView
                android:id="@+id/fengmian_ifv"
                style="@style/add_item_img_style"
                android:src="@mipmap/upload"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="服务类型"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/fuwuleixing_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择服务类型"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/fuwujiage_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="服务价格"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/fuwujiage_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="服务价格"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="预约类型"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/yuyueleixing_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择预约类型"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="预约时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/yuyueshijian_tv"
            android:text="请选择预约时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangmendizhi_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="上门地址"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangmendizhi_et"
            style="@style/add_item_input_style"
            android:hint="上门地址"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/beizhu_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="备注"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/beizhu_et"
            style="@style/add_item_input_style"
            android:hint="备注"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yonghuzhanghao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="用户账号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yonghuzhanghao_et"
            style="@style/add_item_input_style"
            android:hint="用户账号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yonghuxingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="用户姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yonghuxingming_et"
            style="@style/add_item_input_style"
            android:hint="用户姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shoujihaoma_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="手机号码"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shoujihaoma_et"
            style="@style/add_item_input_style"
            android:hint="手机号码"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/dianpumingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="店铺名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/dianpumingcheng_et"
            style="@style/add_item_input_style"
            android:hint="店铺名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/fuwurenyuan_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="服务人员"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/fuwurenyuan_et"
            style="@style/add_item_input_style"
            android:hint="服务人员"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="完成时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/wanchengshijian_tv"
            android:text="请选择完成时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFrameLayout
        style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>


    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
